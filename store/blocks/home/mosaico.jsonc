        "props": {
            "blockClass":"image__mosaico",
            "src": "assets/mosaico-topo/lavanderias-e-areas-externas.png",
            "link": {
              "url": "/lavanderia-e-area-externa"
            }
        }
    },

    // IMAGEM 3
    "flex-layout.col#mosaico_three":{
        "props": {
            "verticalAlign": "middle",
