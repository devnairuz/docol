{
    "flex-layout.row#mosaicos_topo": {
        "title": "Mosaico - Topo",
        "props": {"blockClass": ["wrapper__mosaicos", "wrapper__mosaicos--topo"]},
        "children": [
            "flex-layout.col#mosaico_one",
            "flex-layout.col#mosaico_two",
            "flex-layout.col#mosaico_three"
        ]
    },
    // IMAGEM 1
    "flex-layout.col#mosaico_one":{
        "props": {
            "verticalAlign": "middle",
            "horizontalAlign": "center",
            "blockClass":["wrapper__mosaico"]
        },
        "children": ["image#one"]
    },
    "image#one":{
        "props": {
            "blockClass":"image__mosaico",
