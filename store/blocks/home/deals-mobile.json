{
  "flex-layout.row#dealsmob": {
    "children": ["flex-layout.col#benemob"]
  },

  "flex-layout.col#benemob": {
    "children": ["slider-layout#listabene"],
    "props": {
      "width": "100%"
    }
  },

  "slider-layout#listabene": {
    "children": [
      "flex-layout.col#deals1",
      "flex-layout.col#deals2",
      "flex-layout.col#deals3",
      "flex-layout.col#deals4",
      "flex-layout.col#deals5"
    ],
    "props": {
      "blockClass": "sliderbene",
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": false
      },
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 3,
        "phone": 1
      },
      "infinite": true
    }
  }
}
